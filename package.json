    "debug": "~2.6.9",
    "electron": "^4.1.4",
    "express": "~4.16.0",
    "mongoose": "^5.5.3",
    "morgan": "^1.9.1",
    "nightmare": "^3.0.1",
    "node-cron": "^2.0.3"
  },
  "devDependencies": {
    "@babel/cli": "^7.4.3",
    "@babel/core": "^7.4.3",
    "@babel/node": "^7.2.2",
    "@babel/plugin-proposal-class-properties": "^7.4.0",
    "@babel/preset-env": "^7.4.3",
    "@babel/register": "^7.4.0",
    "babel-eslint": "^10.0.1",
    "core-js": "^3.0.1",
    "eslint": "^5.16.0",
    "eslint-plugin-node": "^8.0.1",
    "nodemon": "^1.18.11"
